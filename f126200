prompt --application/deployment/install/install_script_web
begin
wwv_flow_imp_shared.create_install_script(
 p_id=>wwv_flow_imp.id(51895622767955718399)
,p_install_id=>wwv_flow_imp.id(51894865733024411191)
,p_name=>'script_web'
,p_sequence=>10
,p_script_type=>'INSTALL'
,p_script_clob=>wwv_flow_string.join(wwv_flow_t_varchar2(
'CREATE TABLE "CLIENTE" ',
'   (	"ID_CLIENTE" NUMBER NOT NULL ENABLE, ',
'	"NOMBRE" VARCHAR2(20), ',
'	"APELLIDO" VARCHAR2(20), ',
'	"NRO_DOCUMENTO" VARCHAR2(10), ',
'	"TIPO_DOCUMENTO" NUMBER, ',
'	"NACIONALIDAD" VARCHAR2(15), ',
'	"EMAIL" VARCHAR2(15), ',
'	"TELEFONO" VARCHAR2(15), ',
'	"FEC_NACIMIENTO" DATE, ',
'	 CONSTRAINT "CLIENTE_PK" PRIMARY KEY ("ID_CLIENTE")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "BOLSA_PUNTAJE" ',
'   (	"ID_BOLSA_PUNTAJE" NUMBER NOT NULL ENABLE, ',
'	"ID_CLIENTE" NUMBER, ',
'	"FECHA_ASIGNACION" DATE, ',
'	"FECHA_CADUCIDAD" DATE, ',
'	"PUNTAJE_ASIGNADO" NUMBER, ',
'	"PUNTAJE_UTILIZADO" NUMBER, ',
'	"SALDO_PUNTOS" NUMBER, ',
'	"MONTO_OPERACION" NUMBER, ',
'	 CONSTRAINT "BOLSA_PUNTAJE_PK" PRIMARY KEY ("ID_BOLSA_PUNTAJE")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "USO_PUNTAJE" ',
'   (	"ID_USO_CAB" NUMBER NOT NULL ENABLE, ',
'	"ID_CLIENTE" NUMBER, ',
'	"PUNTAJE_UTILIZADO" NUMBER, ',
'	"FECHA" DATE, ',
'	"CONCEPTO_USO" VARCHAR2(25), ',
'	 CONSTRAINT "USO_PUNTAJE_PK" PRIMARY KEY ("ID_USO_CAB")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "DEPT" ',
'   (	"DEPTNO" NUMBER(2,0) NOT NULL ENABLE, ',
'	"DNAME" VARCHAR2(50), ',
'	"LOC" VARCHAR2(50), ',
'	 CONSTRAINT "DEPT_PK" PRIMARY KEY ("DEPTNO")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "EMP" ',
'   (	"EMPNO" NUMBER(4,0) NOT NULL ENABLE, ',
'	"ENAME" VARCHAR2(50), ',
'	"JOB" VARCHAR2(50), ',
'	"MGR" NUMBER(4,0), ',
'	"HIREDATE" DATE, ',
'	"SAL" NUMBER(7,2), ',
'	"COMM" NUMBER(7,2), ',
'	"DEPTNO" NUMBER(2,0), ',
'	 CONSTRAINT "EMP_PK" PRIMARY KEY ("EMPNO")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "DET_USO_PUNTAJE" ',
'   (	"ID_DET_USO" NUMBER NOT NULL ENABLE, ',
'	"ID_USO_CAB" NUMBER, ',
'	"PUNTAJE_UTILIZADO" NUMBER, ',
'	"ID_BOLSA_PUNTAJE" NUMBER, ',
'	 CONSTRAINT "DET_USO_PUNTAJE_PK" PRIMARY KEY ("ID_DET_USO")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "LIM_PUNTAJE" ',
'   (	"ID_LIM" NUMBER NOT NULL ENABLE, ',
'	"LIMITE_INFERIOR" NUMBER, ',
'	"LIMITE_SUPERIOR" NUMBER, ',
'	"MONTO_EQUIVALENCIA" NUMBER, ',
'	 CONSTRAINT "LIM_PUNTAJE_PK" PRIMARY KEY ("ID_LIM")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "CONCEPTO_USO_PUNTO" ',
'   (	"ID_CON_USO_PUNTO" NUMBER NOT NULL ENABLE, ',
'	"DESCRIPCION" VARCHAR2(50), ',
'	"PUNTOS_REQUERIDOS" NUMBER, ',
'	 CONSTRAINT "CONCEPTO_USO_PUNTO_PK" PRIMARY KEY ("ID_CON_USO_PUNTO")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "PUNTAJE" ',
'   (	"ID_PUNTAJE" NUMBER NOT NULL ENABLE, ',
'	"DESCRIPCION_CONCEPTO" VARCHAR2(25), ',
'	"PUNTOS_REQUERIDOS" NUMBER, ',
'	 CONSTRAINT "PUNTAJE_PK" PRIMARY KEY ("ID_PUNTAJE")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'CREATE TABLE "VENCIMIENTO_PUNTAJE" ',
'   (	"ID_VENCIMIENTO" NUMBER NOT NULL ENABLE, ',
'	"FECHA_INICIO" DATE, ',
'	"FECHA_FIN" DATE, ',
'	"DIA_DURACION_PUNTAJE" NUMBER, ',
'	 CONSTRAINT "VENCIMIENTO_PUNTAJE_PK" PRIMARY KEY ("ID_VENCIMIENTO")',
'  USING INDEX  ENABLE',
'   ) ;',
'',
'ALTER TABLE "BOLSA_PUNTAJE" ADD CONSTRAINT "BOLSA_PUNTAJE_FK" FOREIGN KEY ("ID_CLIENTE")',
'	  REFERENCES "CLIENTE" ("ID_CLIENTE") ENABLE;',
'',
'ALTER TABLE "DET_USO_PUNTAJE" ADD CONSTRAINT "DET_USO_BOLSA_PUNTAJE_FK" FOREIGN KEY ("ID_BOLSA_PUNTAJE")',
'	  REFERENCES "BOLSA_PUNTAJE" ("ID_BOLSA_PUNTAJE") ENABLE;',
'',
'ALTER TABLE "DET_USO_PUNTAJE" ADD CONSTRAINT "DET_USO_PUNTAJE_FK" FOREIGN KEY ("ID_USO_CAB")',
'	  REFERENCES "USO_PUNTAJE" ("ID_USO_CAB") ENABLE;',
'',
'ALTER TABLE "EMP" ADD CONSTRAINT "EMP_DEPT_FK" FOREIGN KEY ("DEPTNO")',
'	  REFERENCES "DEPT" ("DEPTNO") ENABLE;',
'',
'ALTER TABLE "EMP" ADD CONSTRAINT "EMP_MGR_FK" FOREIGN KEY ("MGR")',
'	  REFERENCES "EMP" ("EMPNO") ENABLE;',
'',
'ALTER TABLE "USO_PUNTAJE" ADD CONSTRAINT "USO_PUNTAJE_FK" FOREIGN KEY ("ID_CLIENTE")',
'	  REFERENCES "CLIENTE" ("ID_CLIENTE") ENABLE;',
'',
'CREATE INDEX "EMP_1" ON "EMP" ("MGR") ',
'  ;',
'',
'CREATE INDEX "EMP_2" ON "EMP" ("DEPTNO") ',
'  ;',
'',
' CREATE SEQUENCE  "BOLSA_PUNTAJE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "CLIENTE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 61 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "CONCEPTO_USO_PUNTO_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "DEPT_SEQ"  MINVALUE 1 MAXVALUE 9000 INCREMENT BY 10 START WITH 50 NOCACHE  NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "DET_USO_PUNTAJE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "EMP_SEQ"  MINVALUE 1 MAXVALUE 90000 INCREMENT BY 10 START WITH 8000 NOCACHE  NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "LIM_PUNTAJE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "PUNTAJE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 1 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "USO_PUNTAJE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 41 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
' CREATE SEQUENCE  "VENCIMIENTO_PUNTAJE_SEQ"  MINVALUE 1 MAXVALUE 9999999999999999999999999999 INCREMENT BY 1 START WITH 21 CACHE 20 NOORDER  NOCYCLE  NOKEEP  GLOBAL ;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_BOLSA_PUNTAJE" ',
'  before insert on "BOLSA_PUNTAJE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_BOLSA_PUNTAJE" is null then ',
'    select "BOLSA_PUNTAJE_SEQ".nextval into :NEW."ID_BOLSA_PUNTAJE" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_BOLSA_PUNTAJE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_CLIENTE" ',
'  before insert on "CLIENTE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_CLIENTE" is null then ',
'    select "CLIENTE_SEQ".nextval into :NEW."ID_CLIENTE" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_CLIENTE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_CONCEPTO_USO_PUNTO" ',
'  before insert on "CONCEPTO_USO_PUNTO"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_CON_USO_PUNTO" is null then ',
'    select "CONCEPTO_USO_PUNTO_SEQ".nextval into :NEW."ID_CON_USO_PUNTO" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_CONCEPTO_USO_PUNTO" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_DET_USO_PUNTAJE" ',
'  before insert on "DET_USO_PUNTAJE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_DET_USO" is null then ',
'    select "DET_USO_PUNTAJE_SEQ".nextval into :NEW."ID_DET_USO" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_DET_USO_PUNTAJE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_LIM_PUNTAJE" ',
'  before insert on "LIM_PUNTAJE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_LIM" is null then ',
'    select "LIM_PUNTAJE_SEQ".nextval into :NEW."ID_LIM" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_LIM_PUNTAJE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_PUNTAJE" ',
'  before insert on "PUNTAJE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_PUNTAJE" is null then ',
'    select "PUNTAJE_SEQ".nextval into :NEW."ID_PUNTAJE" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_PUNTAJE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_USO_PUNTAJE" ',
'  before insert on "USO_PUNTAJE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_USO_CAB" is null then ',
'    select "USO_PUNTAJE_SEQ".nextval into :NEW."ID_USO_CAB" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_USO_PUNTAJE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "BI_VENCIMIENTO_PUNTAJE" ',
'  before insert on "VENCIMIENTO_PUNTAJE"               ',
'  for each row  ',
'begin   ',
'  if :NEW."ID_VENCIMIENTO" is null then ',
'    select "VENCIMIENTO_PUNTAJE_SEQ".nextval into :NEW."ID_VENCIMIENTO" from sys.dual; ',
'  end if; ',
'end; ',
'',
'/',
'',
'',
'ALTER TRIGGER "BI_VENCIMIENTO_PUNTAJE" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "DEPT_TRG1" ',
'    before insert on dept',
'    for each row',
'begin',
'    if :new.deptno is null then',
'        select dept_seq.nextval into :new.deptno from sys.dual;',
'    end if;',
'end;',
'/',
'',
'',
'ALTER TRIGGER "DEPT_TRG1" ENABLE;',
'',
'CREATE OR REPLACE EDITIONABLE TRIGGER "EMP_TRG1" ',
'    before insert on emp',
'    for each row',
'begin',
'    if :new.empno is null then',
'        select emp_seq.nextval into :new.empno from sys.dual;',
'    end if;',
'end;',
'/',
'',
'',
'ALTER TRIGGER "EMP_TRG1" ENABLE;',
'',
'CREATE OR REPLACE FORCE EDITIONABLE VIEW "EMP_DEPT_V" ("EMPNO", "ENAME", "JOB", "MGR", "HIREDATE", "SAL", "COMM", "DEPTNO", "DNAME", "LOC") AS ',
'  select e.empno',
',      e.ename',
',      e.job',
',      (select m.ename from emp m where e.mgr = m.empno) mgr',
',      e.hiredate',
',      e.sal',
',      e.comm',
',      d.deptno',
',      d.dname',
',      d.loc',
'from emp e',
',    dept d',
'where e.deptno = d.deptno (+);',
'',
''))
);
