wwv_flow_imp_page.create_report_region(
 p_id=>wwv_flow_imp.id(50810541831503222996)
,p_name=>'Registros maestros'
,p_template=>wwv_flow_imp.id(50414471762314977785)
,p_display_sequence=>20
,p_region_css_classes=>'search-results'
,p_region_template_options=>'#DEFAULT#'
,p_component_template_options=>'t-MediaList--showDesc:t-MediaList--stack'
,p_display_point=>'REGION_POSITION_02'
,p_source_type=>'NATIVE_SQL_REPORT'
,p_query_type=>'SQL'
,p_source=>wwv_flow_string.join(wwv_flow_t_varchar2(
'select "ID_USO_CAB",',
'    null LINK_CLASS,',
'    apex_page.get_url(p_items => ''P28_ID_USO_CAB'', p_values => "ID_USO_CAB") LINK,',
'    null ICON_CLASS,',
'    null LINK_ATTR,',
'    null ICON_COLOR_CLASS,',
'    case when coalesce(:P28_ID_USO_CAB,''0'') = "ID_USO_CAB"',
'      then ''is-active'' ',
'      else '' ''',
'    end LIST_CLASS,',
'    (substr("CONCEPTO_USO", 1, 50)||( case when length("CONCEPTO_USO") > 50 then ''...'' else '''' end )) LIST_TITLE,',
'    (substr("FECHA", 1, 50)||( case when length("FECHA") > 50 then ''...'' else '''' end )) LIST_TEXT,',
'    null LIST_BADGE',
'from "USO_PUNTAJE" x',
'where (:P28_SEARCH is null',
'        or upper(x."CONCEPTO_USO") like ''%''||upper(:P28_SEARCH)||''%''',
'        or upper(x."FECHA") like ''%''||upper(:P28_SEARCH)||''%''',
'    )',
'order by "CONCEPTO_USO"'))
,p_ajax_enabled=>'Y'
,p_ajax_items_to_submit=>'P28_SEARCH'
,p_lazy_loading=>false
,p_query_row_template=>wwv_flow_imp.id(50414565518281977820)
,p_query_num_rows=>1000
,p_query_options=>'DERIVED_REPORT_COLUMNS'
,p_query_no_data_found=>unistr('<div class="u-tC">No se ha encontrado ning\00FAn dato.</div>')
,p_query_row_count_max=>500
,p_csv_output=>'N'
,p_prn_output=>'N'
,p_sort_null=>'L'
,p_plug_query_strip_html=>'N'
);
